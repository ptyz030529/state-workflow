{
    "name": "ptyz030529/state-workflow",
    "type": "library",
    "description": "State Management workflow for Laravel, this package is fork form RingierIMU/state-workflow",
    "keywords": [
        "workflow",
        "state management",
        "laravel"
    ],
    "homepage": "https://github.com/RingierIMU/state-workflow",
    "license": "MIT",
    "authors": [
        {
            "name": "JimbaCheng",
            "email": "ptyz030529@sina.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">7.2.5",
        "illuminate/events": ">7.0",
        "illuminate/support": ">7.0",
        "symfony/event-dispatcher": "^5.1",
        "symfony/workflow": "^5.1",
        "symfony/property-access": "^5.1"
    },
    "require-dev": {
        "funkjedi/composer-include-files": "^1.0",
        "mockery/mockery": "^1.3",
        "orchestra/database": "5.*",
        "orchestra/testbench": "5.*",
        "phpunit/phpunit": "^8.0"
    },
    "extra": {
        "include_files": [
            "tests/Fixtures/Helpers.php"
        ],
        "laravel": {
            "providers": [
                "ptyz030529\\StateWorkflow\\StateWorkflowServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "ptyz030529\\StateWorkflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ptyz030529\\StateWorkflow\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/Fixtures/database/factories"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
